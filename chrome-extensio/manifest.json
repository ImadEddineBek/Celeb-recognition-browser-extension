{
  "name": "Who's this?",
  "description": "A service made by BEKKOUCH to help you identify the person whos name you just entered",
  "version": "1.0",
  "manifest_version": 2,
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "/images/logo.png"
    }
  },
  "icons": {
    "128": "/images/logo.png"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "downloads",
    "storage",
    "activeTab",
    "declarativeContent"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "options_page": "options.html"
}
